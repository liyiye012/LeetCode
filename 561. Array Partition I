class Solution {
public:
    int arrayPairSum(vector<int>& nums) {
        int max=0;
        sort(nums.begin(),nums.end());
        // for(int i=0;i<nums.size();i++){
        //     cout<<nums[i]<<" ";
        // }
        for(int i=0;i<nums.size();i=i+2){
            max=max+ nums[i];
            // cout<<"i="<<i<<endl;
        }
        return max;
    }
};
