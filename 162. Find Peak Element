class Solution {
public:
    int findPeakElement(vector<int>& nums) {
        for(int i=0;i<nums.size()-1;i++){
            if(nums[i]>nums[i+1]){ 
             return i;
            cout<<"We have returned i="<<i<<endl;
            }
        }
      //        if(nums.size()==1) return 0;
        return nums.size()-1;
    }
};
