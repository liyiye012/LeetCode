class Solution {
public:
    vector<string> fizzBuzz(int n) {
        vector<string> str(n);
        for(int i=0;i<n;i++){
         if((i+1)%3==0) str[i]+="Fizz";
         if((i+1)%5==0) str[i]+="Buzz";
         if((i+1)%3!=0&&(i+1)%5!=0) str[i]+=to_string(i+1);         
            
        }
        return str;
        
        
    }
};
