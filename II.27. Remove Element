class Solution {
public:
      void output(vector<int> nums){
      for(int i=0;i<nums.size();i++){
            cout<<nums[i]<<" ";
       }
        cout<<endl;
    }
    int removeElement(vector<int>& nums, int val) {
     int begin=0;
        for(int i=0;i<nums.size();i++){
            output(nums);
            if(nums[i]!=val){
                nums[begin]=nums[i];
                begin++;
            }
        }
     return begin;
    }
};
